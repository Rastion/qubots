{
  "type": "optimizer",
  "entry_point": "qubot",
  "class_name": "PyomoMaxCutOptimizer",
  "default_params": {
    "solver_name": "gurobi",
    "time_limit": 300.0,
    "mip_gap": 0.01,
    "threads": 0
  },
  "metadata": {
    "name": "Pyomo MaxCut Optimizer",
    "description": "Flexible solver for Maximum Cut problems using Pyomo modeling framework. Supports multiple solver backends including Gurobi, CPLEX, CBC, and GLPK for maximum compatibility and flexibility.",
    "domain": "graph_theory",
    "tags": [
      "pyomo",
      "exact",
      "maxcut",
      "quadratic",
      "binary",
      "optimization",
      "mathematical_programming",
      "multi_solver"
    ],
    "difficulty": "intermediate",
    "optimizer_type": "exact"
  },
  "parameters": {
    "solver_name": {
      "type": "string",
      "default": "gurobi",
      "options": ["gurobi", "cplex", "cbc", "glpk", "ipopt"],
      "description": "Backend solver to use for optimization"
    },
    "time_limit": {
      "type": "number",
      "default": 300.0,
      "min": 1.0,
      "max": 3600.0,
      "description": "Maximum solving time in seconds"
    },
    "mip_gap": {
      "type": "number",
      "default": 0.01,
      "min": 0.0001,
      "max": 0.5,
      "description": "Relative optimality gap tolerance (0.01 = 1%)"
    },
    "threads": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "max": 16,
      "description": "Number of threads to use (0 = automatic)"
    },
    "tee": {
      "type": "boolean",
      "default": false,
      "description": "Enable solver output streaming to console"
    },
    "solver_options": {
      "type": "object",
      "default": {},
      "description": "Additional solver-specific options as key-value pairs"
    }
  }
}
